<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd">
<suite name="SmokeSuite" thread-count="1" parallel="tests">
	
	<listeners>
		<listener class-name="ExtentReports.ExtentTestNGIReporterListener"></listener>
	</listeners>
	
  <test  name="ChromeTest">
	  <parameter name="browser" value="Chrome"></parameter>
    <classes>
      <class name="TestNG.VerifySearchButtonOfMainPage"/>
    </classes>
  </test> 
  
  <test  name="EdgeTest">
	  <parameter name="browser" value="Edge"></parameter>
    <classes>
      <class name="TestNG.VerifySearchButtonOfMainPage"/>
    </classes>
  </test> 
</suite> 